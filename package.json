{
	"name": "vscode-createfromusage",
	"displayName": "Create from usage",
	"description": "Automatically generates a variable declaration based on TypeScript type information.",
	"version": "0.1.0",
	"publisher": "mape",
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#252627",
		"theme": "dark"
	},
	"license": "MIT",
	"licenseUrl": "LICENSE",
	"engines": {
		"vscode": "^1.9.0"
	},
	"categories": [
		"Other",
		"Snippets",
		"Languages"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "createFromUsage.createVariable",
				"title": "Create from usage"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "createFromUsage.createVariable"
				}
			]
		},
		"keybindings": [
			{
				"command": "createFromUsage.createVariable",
				"key": "Ctrl+Up"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Create from usage configuration",
			"properties": {
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"lint": "node ./node_modules/tslint/bin/tslint src/**/*.ts",
		"compile": "npm run lint && tsc -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/node": "^6.0.59",
		"tslint": "^4.3.1",
		"vscode": "^1.0.3"
	},
	"dependencies": {
		"typescript": "^2.2.0"
	},
	"bugs": {
		"url": "https://github.com/mape/vscode-createFromUsage/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mape/vscode-createFromUsage"
	}
}
